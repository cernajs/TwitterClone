@model List<Tweet>

<link rel="stylesheet" href="~/css/listHeader.css" asp-append-version="true" />

@if(Model.Count() == 0)
{
	<div class="alert alert-info">
        You have no Bookamrks!
    </div>
}

<ul id="tweetFeed">
    @foreach(var tweet in Model)
    {
        <li>
            <partial name="_TweetPartial" model="@tweet" />
        </li>
    }
</ul>